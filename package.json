{
  "name": "tensfer_backend",
  "version": "1.0.0",
  "description": "> Express REST API with JWT Authentication and support for sqlite, mysql, and postgresql using knex as database ORM and objection as knex Model Mapper",
  "main": "src/index",
  "scripts": {
    "postinstall": "npm run link-modules",
    "link-modules": "which link-module-alias && link-module-alias || echo",
    "preinstall": "which link-module-alias && link-module-alias clean || echo",
    "predev": "cross-env NODE_ENV=development; npm run link-modules",
    "migrate": "knex migrate:latest --cwd=src/config",
    "postmigrate": "knex seed:run  --cwd=src/config",
    "dev": "npm run migrate && npm run watch_dev",
    "watch_dev": "nodemon",
    "format": "prettier --write \"**/*.{js, jsx, ts, tsx, json}\"",
    "test": "npm run migrate && jest",
    "test:coverage": "npm test --coverage",
    "pretest": "npm run create-sqlite-db; cross-env NODE_ENV=test; npm run link-modules",
    "posttest": "npm run drop-sqlite-db",
    "drop-sqlite-db": "shx rm ./db/database.sqlite",
    "create-sqlite-db": "shx mkdir db; shx touch db/database.sqlite",
    "report-coverage": "coveralls < ./coverage/lcov.info"
  },
  "keywords": [
    "money",
    "payment",
    "resource manager"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Tensfer-Tech/Tensfer-Backend.git"
  },
  "homepage": "https://github.com/Tensfer-Tech/Tensfer-Backend#readme",
  "bugs": {
    "url": "https://github.com/Tensfer-Tech/Tensfer-Backend/issues"
  },
  "license": "ISC",
  "author": "Blessing Krofegha",
  "_moduleAliasIgnoreWarning": true,
  "_moduleAliases": {
    "@typings": "typings",
    "@seeds": "src/seeds",
    "@routes": "src/routes",
    "@models": "src/models",
    "@config": "src/config",
    "@helpers": "src/helpers",
    "@policies": "src/policies",
    "@services": "src/services",
    "@migrations": "src/migrations",
    "@validations": "src/validations",
    "@controllers": "src/controllers",
    "@middlewares": "src/middlewares",
    "@knexfile": "src/config/knexfile.ts"
  },
  "engines": {
    "node": ">=12"
  },
  "dependencies": {
    "all": "^0.0.0",
    "bcrypt": "^5.0.0",
    "bluebird": "^3.7.2",
    "body-parser": "^1.19.0",
    "celebrate": "^13.0.4",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "helmet": "^4.4.1",
    "http-errors": "^1.8.0",
    "http-status": "^1.5.0",
    "jsonwebtoken": "^8.5.1",
    "knex": "^0.21.17",
    "link-module-alias": "^1.2.0",
    "method-override": "^3.0.0",
    "morgan": "^1.10.0",
    "objection": "^2.2.14",
    "objection-guid": "^3.0.2",
    "objection-unique": "^1.2.1",
    "path": "^0.12.7",
    "pg": "^8.5.1",
    "pg-hstore": "^2.3.3",
    "sqlite3": "^5.0.1",
    "ts-node": "^9.1.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.10",
    "@babel/core": "^7.12.10",
    "@babel/node": "^7.12.10",
    "@babel/preset-env": "^7.12.11",
    "@babel/preset-typescript": "^7.12.7",
    "@hapi/joi": "^17.1.1",
    "@types/bcrypt": "^3.0.0",
    "@types/bluebird": "^3.5.33",
    "@types/compression": "^1.7.0",
    "@types/cookie-parser": "^1.4.2",
    "@types/cors": "^2.8.9",
    "@types/debug": "^4.1.5",
    "@types/express": "^4.17.11",
    "@types/hapi__joi": "^17.1.6",
    "@types/helmet": "^4.0.0",
    "@types/http-errors": "^1.8.0",
    "@types/jest": "^26.0.20",
    "@types/joi": "^17.2.3",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/method-override": "^0.0.31",
    "@types/mongoose": "^5.10.3",
    "@types/morgan": "^1.9.2",
    "@types/node": "^14.14.22",
    "@types/nodemailer": "^6.4.0",
    "@types/nodemailer-mailgun-transport": "^1.4.2",
    "@types/supertest": "^2.0.10",
    "babel-jest": "^26.6.3",
    "debug": "^4.3.1",
    "eslint": "^7.19.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^7.2.0",
    "husky": "^4.3.8",
    "jest": "^26.6.3",
    "nodemon": "^2.0.7",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.2.1",
    "rimraf": "^3.0.2",
    "shx": "^0.3.3",
    "supertest": "^6.1.3",
    "ts-jest": "^26.5.0",
    "tsc-watch": "^4.2.9",
    "typescript": "^4.1.3"
  }
}
